{
    "suggest.enablePreselect": true,
    "suggest.autoTrigger": "always",
    "suggest.fixInsertedWord": false,
    "suggest.timeout": 500,
    "diagnostic.level": "hint",
    "diagnostic.messageDelay": 100,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.maxWindowHeight": 20,
    "suggest.triggerCompletionWait": 30,
    "rust-analyzer.updates.channel": "nightly",
    "rust-analyzer.checkOnSave.command": "clippy",
    "rust-analyzer.diagnostics.enableExperimental": false,
    "rust-analyzer.procMacro.enable": true,
    "rust-analyzer.updates.prompt": false,
    "rust-analyzer.inlayHints.chainingHints": true,
    "rust-analyzer.lruCapacity": 512,
    "rust-analyzer.inlayHints.typeHintsSeparator": "┇ ",
    "rust-analyzer.inlayHints.chainingHintsSeparator": "┇ ",
    "rust-analyzer.notifications.cargoTomlNotFound": false,
    "rust-analyzer.cargo.allFeatures": true,
    "rust-analyzer.debug.runtime": "vimspector",
    "coc.preferences.colorSupport": true,
    "coc.preferences.jumpCommand": "edit",
    "coc.preferences.extensionUpdateCheck": "never"
}
